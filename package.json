{
  "name": "@tiny-premium/beehive-cli",
  "version": "0.1.1",
  "main": "index.js",
  "repository": "git@github.com:tinymce/beehive-cli.git",
  "license": "MIT",
  "dependencies": {
    "@types/luxon": "^1.25.0",
    "fp-ts": "^2.8.4",
    "kind-of": "^6.0.3",
    "luxon": "^1.25.0",
    "properties-reader": "^2.1.1",
    "read-pkg": "^5.2.0",
    "simple-git": "^2.21.0",
    "tmp": "^0.2.1",
    "tslib": "^2.0.3",
    "yargs": "^16.0.3"
  },
  "scripts": {
    "beehive-flow": "./src/main/ts/Main.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "testOnly": "nyc mocha -r esm --require ts-node/register --require source-map-support/register --recursive --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi-reporters-config.json src/test/ts/**/*Test.ts",
    "buildTestOnly": "yarn build && yarn testOnly",
    "test": "yarn build && yarn lint && yarn testOnly",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@tinymce/eslint-plugin": "^1.4.1",
    "@types/chai": "^4.2.13",
    "@types/chai-as-promised": "^7.1.3",
    "@types/kind-of": "^6.0.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/properties-reader": "^2.1.0",
    "@types/tmp": "^0.2.0",
    "@types/yargs": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "esm": "^3.2.25",
    "fast-check": "^2.5.0",
    "mocha": "^8.1.3",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.1.7",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "publishConfig": {
    "@tiny-premium:registry": "https://npm.cloudsmith.io/tiny/tiny-premium/"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  }
}
